{
  "name": "react-styleable",
  "version": "2.0.0",
  "description": "Higher-order component for styling react",
  "main": "index.js",
  "scripts": {
    "test": "mocha \"src/**/*.spec.js\"",
    "test-watch": "mocha \"src/**/*.spec.js\" --watch",
    "start": "webpack-dev-server --config webpack.config.js",
    "compile": "babel src/ --stage 0 --source-maps --debug --out-dir lib/",
    "prepublish": "npm run compile"
  },
  "keywords": [
    "react",
    "style",
    "react-style",
    "local",
    "css",
    "icss",
    "module",
    "inline styles"
  ],
  "author": "jaketrent",
  "license": "MIT",
  "dependencies": {
    "invariant": "^2.1.0",
    "lodash": "^3.9.3",
    "react": "^0.14.0"
  },
  "devDependencies": {
    "babel": "^5.6.4",
    "jsdom": "^3.1.2",
    "mocha": "^2.3.3",
    "react-addons-test-utils": "^0.14.0",
    "react-dom": "^0.14.0",
    "rf-release": "^0.4.0",
    "should": "^7.0.1"
  }
}