  ],
  "author": "jaketrent",
  "repository": {
    "type": "git",
    "url": "https://github.com/pluralsight/react-styleable.git"
  },
  "license": "MIT",
  "peerDependencies": {
    "react": "^0.14.0"
  },
  "dependencies": {
    "invariant": "^2.1.0"
  },
  "devDependencies": {
    "babel-cli": "^6.4.5",
    "babel-core": "^6.4.5",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "jsdom": "^3.1.2",
    "mocha": "^2.3.3",
    "react-addons-test-utils": "^0.14.0",
    "react-dom": "^0.14.0",
    "rf-release": "^0.4.0",
    "should": "^7.0.1"
