  },
  "dependencies": {
    "invariant": "^2.1.0",
    "prop-types": "^15.6.2"
  },
  "devDependencies": {
    "babel-cli": "^6.4.5",
    "babel-core": "^6.4.5",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-2": "^6.24.1",
    "jsdom": "^13.0.0",
    "mocha": "^5.2.0",
    "raf": "^3.4.0",
    "react": "^16.6.0",
    "react-dom": "^16.6.0",
    "should": "^13.2.3"
